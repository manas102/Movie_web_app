<div class="container pb-cmnt-container">
    <div class="row">
        <div class="col-md-8">
            <div class="panel panel-info">
                <div class="panel-body">
                    <textarea placeholder="Write your comment here!" form="cmtform" name="comment" style=" resize: none;
                        padding: 20px;
                        height: 130px;
                        width: 100%;
                        border: 1px solid #F2F2F2;"></textarea>
                    <form class="form-inline" id="cmtform" method="POST" enctype="application/x-www-form-urlencoded" action="<%=stuff.Name%>">
                        <button class="btn btn-default pull-right" type="submit">Post</button>
                    </form>
                </div>
            </div>
        </div>
        <div class="col-md-2"></div>
    </div>
</div>
<h1>
    <%var jsonArray = JSON.parse(JSON.stringify(comments))%>
        <%=jsonArray%>
            <h2>Length
                <%=jsonArray.length%>
            </h2>
</h1>